/* xterm */
XTerm*renderFont: true
XTerm*faceName: xft:Monego:size=9, \
                xft:JoyPixels:size=12, \
                xft:Monospace:style=Medium:size=12   
XTerm*faceSize: 11
XTerm*utf8: 2
XTerm*locale: true

